// Block Letters Initials

#include <iostream>

int main() {

  std::cout<< "M   M  RRRR \n";
  std::cout<< "MM MM  R   R\n"; 
  std::cout<< "MM MM  R   R\n";
  std::cout<< "M M M  RRRR \n";
  std::cout<< "M   M  R R  \n";
  std::cout<< "M   M  R  R \n";
  std::cout<< "M   M  R   R\n";
  return 0;

}
